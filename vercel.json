{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/(.+\\.js)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.+\\.css)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.+\\.png|.+\\.jpg|.+\\.ico|.+\\.svg)",
      "dest": "/public/$1"
    },
    {
      "src": "/sw.js",
      "dest": "/public/sw.js"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
